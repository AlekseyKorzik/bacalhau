type WasmEngineSpec struct {
    EntryModule nullable StorageSpec
    EntryPoint String
    Parameters [String]
    EnvironmentVariables [String]
    ImportModules [StorageSpec]
}

type StorageSpec struct {
    Type String
    Schema Link
    SchemaData Bytes
    Params Bytes

    Name String
    Mount String
    Metadata {String:String}
}